@import "reset.styl"

.layout
  width 1440px
  height: 5000px
  margin 0 auto
  overflow hidden

.photo
  height 813px
  object-fit contain
  background-repeat no-repeat
  background-image url("../img/photo1.jpg")

.thereisno
  width 800px
  margin 0 auto
  padding 180px 0 0
  font-family Exo
  font-size 60px
  font-weight 100
  font-stretch normal
  font-style normal
  line-height 74px
  letter-spacing 2.4px
  text-align center
  color #282828   

.circle
  width 59px
  height 59px
  margin 60px auto 
  text-align center
  border-radius 50px
  background-color #8286a5  
  background-image url("../img/playbutt.png")
  background-repeat no-repeat
  background-position 23px 20px
  cursor pointer

.button
  width 190px
  height 50px
  margin -10px 630px 
  border-radius 3px
  background-color #ff6d6d
  border none
  cursor pointer

.button:active  
  color red 

.texttrynow
  font-family Exo
  font-size 16px
  font-weight normal
  font-stretch normal
  font-style normal
  line-height 20px
  text-transform uppercase
  letter-spacing 3.2px
  text-align center
  color #ffffff 

.textnoneed
  margin 30px
  font-family Exo
  font-size 14px
  font-weight normal
  font-stretch normal
  font-style normal
  letter-spacing 0.56px
  text-align center
  color #828283  




  
